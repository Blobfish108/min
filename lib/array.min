Array do:
  # alias(@, at)
  
  first  = method: self.at(0)
  second = method: self.at(1)
  last   = method: self.at(self.size - 1)
  
  select = method:
    selected = Array.new
    self.each(i):
      if (call.args.first.eval_on(i)):
        selected.push(i)
    selected
  
  map = method:
    values = Array.new
    self.each(i):
      values.push(call.args.first.eval_on(i))
    values